{
  "name": "shiftshop-pos",
  "productName": "shiftshop-pos",
  "version": "1.0.0-RELEASE",
  "description": "ShiftShop Point of Sale App",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "makeLinux": "electron-forge make --platform=linux --arch=x64",
    "makeWindows": "electron-forge make --platform=win32 --arch=x64",
    "makeMacOs": "electron-forge make --platform=darwin --arch=x64",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Marcos Rial Troncoso",
    "email": "marcos.rial@udc.es"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/icons/logo"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "shiftshop_pos"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@electron-forge/cli": "^6.0.0-beta.52",
    "@electron-forge/maker-deb": "^6.0.0-beta.52",
    "@electron-forge/maker-rpm": "^6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.52",
    "@electron-forge/maker-zip": "^6.0.0-beta.52",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.52",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.4.2",
    "devtron": "^1.4.0",
    "electron": "^9.4.0",
    "file-loader": "^6.0.0",
    "node-loader": "^1.0.0",
    "node-sass": "^4.14.1",
    "redux-devtools-extension": "^2.13.8",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.48",
    "@material-ui/pickers": "^3.2.10",
    "base32-encode": "^1.1.1",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "date-fns": "^2.12.0",
    "electron-squirrel-startup": "^1.0.0",
    "notistack": "^0.8.9",
    "pouchdb": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-intl": "^3.9.0",
    "react-load-script": "0.0.6",
    "react-number-format": "^4.4.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5",
    "reselect": "^4.0.0",
    "uuid": "^7.0.3"
  }
}
